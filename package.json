{
  "name": "@grandlinex/bundle-postgresql",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "scripts": {
    "buildprep": "npm run build-mjs && npm run build-cjs && npm run build-fix",
    "build-mjs": "tsc",
    "build-cjs": "tsc -p tsconfig-cjs.json",
    "build-fix": "node ./node_modules/@grandlinex/core/fix.js",
    "lint": "eslint src",
    "test": "jest --runInBand ",
    "run": "ts-node tests/run.ts",
    "test-converage": "jest --runInBand --ci --collectCoverage   --coverageDirectory=\"./coverage\"  --reporters=default --reporters=jest-junit",
    "makeDocs": "typedoc"
  },
  "keywords": [
    "typescript",
    "framework",
    "bundle",
    "grandlinex",
    "postgresql",
    "pg"
  ],
  "author": {
    "name": "Elschnagoo"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@grandlinex/core": "1.0.0",
    "pg": "8.12.0",
    "@types/pg": "8.11.6"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.14.5",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.3.2",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typedoc": "0.25.13",
    "typescript": "5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrandlineX/bundle-postgresql"
  }
}
